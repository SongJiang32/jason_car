                                                                        <!-- gazebo plugin for LIDAR -->
  <gazebo reference="lidar_link">
    <sensor type="ray" name="head_hokuyo_sensor">
      <pose>0 0 -0.03 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>15</update_rate> 
      <ray>
        <scan>
          <horizontal>
            <!-- 水平角分辨率LDS-E300-E: 0.5°@25HZ -->
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-3.1415926535</min_angle>
            <!-- <min_angle>-1.570796</min_angle> -->
            <max_angle>3.1415926535</max_angle>
            <!-- <max_angle>1.570796</max_angle> -->
          </horizontal>
        </scan>
        <range>
          <!-- 0.1-25m（90%） -->
          <min>0.21</min>
          <max>25.0</max>
          <!-- 距离分辨率mm级 -->
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
                achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
                stddev of 0.01m will put 99.7% of samples within 0.03m of the true
                reading. -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <!-- GPU Laser -->
      <!-- <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_gpu_laser.so"> -->
        <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>/scan</topicName>
        <frameName>lidar_link</frameName>
      </plugin>
    </sensor>
  </gazebo>  


                                                                        <!-- gazebo plugin for fcamera -->
 <gazebo reference="fcamera_link">
  <sensor type="depth" name="camera">
    <always_on>true</always_on>
    <update_rate>90.0</update_rate>  <!-- 根据深度分辨率的帧率 -->
    <camera>
      <!-- 深度相机参数 -->
      <horizontal_fov>1.50098</horizontal_fov>  <!-- 86° -> 1.50098 弧度 -->
      <image>
        <format>B8G8R8</format>
        <width>1280</width>
        <height>720</height>
      </image>
      <clip>
        <near>0.3</near>  <!-- 根据理想范围参数 -->
        <far>3.0</far>    <!-- 根据理想范围参数 -->
      </clip>
    </camera>
    <plugin name="camera_plugin" filename="libgazebo_ros_openni_kinect.so">
      <baseline>0.025</baseline>  <!-- 根据相机尺寸估算 -->
      <alwaysOn>true</alwaysOn>
