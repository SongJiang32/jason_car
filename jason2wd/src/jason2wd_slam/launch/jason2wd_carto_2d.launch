<launch>
  <param name="/use_sim_time" value="true" />

  <node name="cartographer_node" pkg="cartographer_ros"
      type="cartographer_node" args="
          -configuration_directory /home/yuanzhishiwo32/cartographer_ws/install_isolated/share/cartographer_ros/configuration_files
          -configuration_basename jason2wd_2d.lua"
      output="screen">
    <remap from="scan" to="scan" />
    <remap from="imu" to="imu/data" />
    <remap from="odom" to="odom" />
  </node>

  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros"
      type="cartographer_occupancy_grid_node" args="-resolution 0.05" />
  <node name="rviz" pkg="rviz" type="rviz" required="true"
      args="-d /home/yuanzhishiwo32/cartographer_ws/src/cartographer_ros/cartographer_ros/configuration_files/jason2wd_2d.rviz" />
 
</launch>
